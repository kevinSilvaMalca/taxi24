  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "cross-env NODE_ENV=dev PORT=3000 nest start --watch",
    "start:dev": "cross-env NODE_ENV=dev PORT=3000 nest start --watch",
    "start:stg": "cross-env NODE_ENV=stg PORT=3000 nest start --watch",
    "start:prd": "cross-env NODE_ENV=prd PORT=3000 nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-esm -d src/infraestructure/adapter/postgress-server.module",
    "migrations:generate": "npm run typeorm -- migration:generate",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "seed": "ts-node ./src/infrastructure/seeds/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
