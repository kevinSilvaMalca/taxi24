    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "exceljs": "^4.3.0",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "mssql": "^9.1.1",
    "nodemailer": "^6.9.4",
    "npm": "^10.0.0",
    "pg": "^8.11.5",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rut.js": "^2.1.0",
